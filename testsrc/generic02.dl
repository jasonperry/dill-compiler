** Actually simpler example than generic01

module generic02 = 

open stdio;

proc (t, u) second(i: t, j: u) => u = 
   return j;
/proc

export proc main() = 
   var x: Int = 42;
   var y: Float = 43.0;
   printFloat(second(x, y));
   printInt(second(y, x));
   printInt(second(x,x));
   printFloat(second(y,y));
   return;
/proc

/module