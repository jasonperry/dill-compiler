(* test of opaque types *)
module opaque01 begin

opaque type Secrets is struct
   x: Int,
   y: Float,
   z: Byte;

type Notsecret is struct
   x: Float,
   y: Float,
   z: Int;

proc setSecret(x: Int, y: Float, z: Byte) => Secrets
begin
   var s: Secrets = {x=x, y=y, z=z};
   return s;
end setSecret
   
proc secretByte(s: Secrets) => Byte
begin
   return s.z;
end secretByte
   
end opaque01

