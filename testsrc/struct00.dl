(* mutable struct initialized with individual field assignments. *)
module struct00 begin

open stdio;

type Complex is struct
  mut i: Float,
  mut j: Float;


export proc main() 
begin

  var cpx: Complex;
  cpx.i = 4.0;
  cpx.j = -1.53;
  printFloat(cpx.i);
  printFloat(cpx.j);

end main

end struct00
