(* Struct passed as argument. *)
module struct04 begin

open stdio;

type Complex is struct
  i: Float,
  j: Float;

proc printComplex(c: Complex) : Void
begin
  printFloat(c.i);
  printFloat(c.j);
  return;
end printComplex


export proc main() : Void 
begin
  var cpx: Complex = {i=2.0, j=1.111};
  printComplex(cpx);

end main

end struct04